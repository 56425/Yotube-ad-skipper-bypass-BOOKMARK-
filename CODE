javascript:(function(){var video=document.querySelector('video');var checkInterval=setInterval(function(){var skipButton=document.querySelector('.ytp-ad-skip-button');var closeButton=document.querySelector('.ytp-ad-overlay-close-button');if(skipButton){skipButton.click();}if(closeButton){closeButton.click();}if(!video.paused&&!video.ended){clearInterval(checkInterval);}},1000);})();
